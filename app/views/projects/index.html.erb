<h1 id="page-title">Projects I've built</h1>

<div id="posts-wrapper" class="skinny-wrapper">
  <% @projects.each do |project| %>
    <div class="post">
      <p class="date"><%= project.created_at.strftime("%b %d, %Y\t%I:%M%p") %></p>
      <h2><a href="#"><%= link_to project.title, project %></a></h2>
      <hr>
    </div>
  <% end %>
</div>
